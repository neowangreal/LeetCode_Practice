class Solution {
    public String reverseWords(String s) {
        // s.trim();
        String[] arr = s.split(" ");
        String output = "";
        for(int i = arr.length - 1; i>=0; i--){
            // System.out.println(arr[i]);
            if(arr[i]!="")
            output += arr[i].trim() + " ";
        }
        return output.trim();
    }
}
