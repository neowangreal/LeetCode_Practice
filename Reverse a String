class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        String s = "";
        char ch;
        for(int i=0; i<str.length(); i++)
        {
            ch = str.charAt(i);
            s = ch + s;
        }
        return s;
    }
    public static void main(String[] args){
        reverseWord("Geeks");
    }
}
